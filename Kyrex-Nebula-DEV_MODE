local Library = loadstring(game:HttpGet("https://raw.githubusercontent.com/KyrexMan/he-e9gh0heyyY31YwEy/refs/heads/main/ageegaegah2yh080gbege2b0bg0e120b8mlkpbfzweq"))()
local Window = Library:CreateWindow({
    Title = "üåÄ " .. '<font color="#51A4E4">Nebula</font>',
    Footer = "üåÄ Nebula | Volleyball Legend | Premium üåÄ",
    Size = UDim2.fromOffset(700, 720),
    IconSize = UDim2.fromOffset(55, 55),
    ToggleKeybind = Enum.KeyCode.K,
    NotifySide = "Right",
    ShowCustomCursor = false,
    Resizable = true,
})

Library:Notify({
    Title = "",
    Description = "Successfully Loaded",
    Time = 4,
})

local Tab1 = Window:AddTab({
    Name = "Player",
    Icon = "user"
})

local Tab2 = Window:AddTab({
    Name = "Physics",
    Icon = "atom"
})

local SpeedGroupBox = Tab2:AddLeftGroupbox("Silent WalkSpeed", "chevrons-right")
local JumpPowerGroupBox = Tab2:AddRightGroupbox("Silent JumpPower", "chevrons-up")

local Tab3 = Window:AddTab({
    Name = "Teleport",
    Icon = "map-pin"
})

local Places = Tab3:AddLeftGroupbox("Places", "map")

local Tab4 = Window:AddTab({
    Name = "Visual",
    Icon = "eye"
})

local Tab5 = Window:AddTab({
    Name = "Protection",
    Icon = "shield-check"
})

local Tab6 = Window:AddTab({
    Name = "Misc",
    Icon = "settings"
})

local HitboxGroupbox = Tab1:AddLeftGroupbox("Hitbox", "box")
 
--===============================================================================================================
                                            -- PLAYER HITBOX
--===============================================================================================================

local Options = Library.Options

-- Add the toggle to enable/disable the effect
local TogglePlayerHitbox = HitboxGroupbox:AddToggle("EnableHitbox", {
    Text = "Player Hitbox",
    Default = false, -- starts OFF
})

-- Add the slider to control size
local SliderPlayerHitbox = HitboxGroupbox:AddSlider("PlayerHitbox", {
    Text = "Player Hitbox Size",
    Default = 9,
    Min = 9,
    Max = 50,
    Rounding = 0,
})

-- Global variables
getgenv().AutoShow = false  -- controlled by the toggle
getgenv().PartSize = SliderPlayerHitbox.Value or 9
local HitboxColor = Color3.fromRGB(81, 164, 228) -- fixed color (red)

-- Update PartSize when slider changes, only if toggle is ON
SliderPlayerHitbox:OnChanged(function(value)
    if getgenv().AutoShow then
        getgenv().PartSize = value
        print("Slider changed to", value)
    end
end)

-- Update toggle
TogglePlayerHitbox:OnChanged(function(state)
    getgenv().AutoShow = state
    print("Hitbox toggle is now", state and "ON" or "OFF")
end)

-- Effect function
local function applyEffect(part)
    task.spawn(function()
        while part and part.Parent do
            if getgenv().AutoShow then
                part.Transparency = 0.45
                part.Shape = Enum.PartType.Ball
                part.Size = Vector3.new(getgenv().PartSize, getgenv().PartSize, getgenv().PartSize)
                part.Material = Enum.Material.ForceField
                part.Color = HitboxColor -- fixed color
            end
            task.wait(0.1)
        end
    end)
end

-- Apply to existing parts
for _, part in ipairs(workspace:GetChildren()) do
    if part.Name == "Part" then
        applyEffect(part)
    end
end

-- Apply to new parts as they are added
workspace.ChildAdded:Connect(function(child)
    if child.Name == "Part" then
        applyEffect(child)
    end
end)



--===============================================================================================================
                                                    -- BALL SIZE
--===============================================================================================================

local BallSizeGroupbox = Tab1:AddRightGroupbox("Ball Size", "volleyball")

--===============================================================================================================
                                                    -- SPEED BOOST
--===============================================================================================================

local Players = game:GetService("Players")
local RunService = game:GetService("RunService")

local player = Players.LocalPlayer
local humanoid
local baseSpeed = game.Players.LocalPlayer.Character.Humanoid.WalkSpeed

-- get humanoid safely
local function setHumanoid(char)
    humanoid = char:WaitForChild("Humanoid")
    baseSpeed = humanoid.WalkSpeed
end

setHumanoid(player.Character or player.CharacterAdded:Wait())
player.CharacterAdded:Connect(setHumanoid)

local speedEnabled = false
local speedAdd = 0

-- Toggle
local SpeedBoostToggle = SpeedGroupBox:AddToggle("SpeedBoostToggle", {
    Text = "Speed Boost",
    Default = true,
})

SpeedBoostToggle:OnChanged(function(state)
    speedEnabled = state
end)

-- Slider (ADD 1 ‚Üí 10)
local WalkSpeedSlider = SpeedGroupBox:AddSlider("WalkSpeedSlider", {
    Text = "WalkSpeed Boost (+)",
    Default = 1,
    Min = 1,
    Max = 10,
    Rounding = 0,
})

WalkSpeedSlider:OnChanged(function(value)
    speedAdd = value
end)

-- üîÅ LOOP (this is the important part)
RunService.Heartbeat:Connect(function()
    if humanoid then
        humanoid.WalkSpeed = baseSpeed + (speedEnabled and speedAdd or 0)
    end
end)
